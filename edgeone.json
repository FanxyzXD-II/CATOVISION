{
  "version": 1,
  "routes": [
    {
      "pattern": "/static/(.*)",
      "target": "/static/$1",
      "type": "static"
    },
    {
      "pattern": "/(.*)",
      "target": "/api/main.py",
      "type": "function"
    }
  ],
  "function_config": {
    "runtime": "python3",
    "memory": 1024,
    "timeout": 60
  }
}
